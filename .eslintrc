{
  "extends": ["acme", "plugin:@next/next/recommended"],
  "settings": {
    "import/resolver": {
      "alias": {
        "map": [["@", "./src"]],
        "extensions": [".js"]
      }
    }
  },
  "rules": {
    "no-unused-vars": "warn",
    "react-hooks/exhaustive-deps": "warn",
    "import/order": "warn"
  }
}
